{
  "manifest_version": 2,
  "name": "employees-helper",
  "description": "A Chrome Extension for tapjoy korea employees",
  "version": "$",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "*",
    "https://*.*.*/*",
    "http://*.tapjoy.com/*",
    "https://*.tapjoy.net/*",
    "http://*.tapjoy.net/*",
    "http://*.tapjoy.localdev/*",
    "http://*.tapjoy.local/*",
    "http://localhost/*"
  ]
}
